{
  "name": "atom-ide-diagnostics",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "A replacement of the diagnostics functionality from the original Atom-IDE / Nuclide package developed by Facebook.",
  "keywords": [
    "atom-package",
    "atom-ide",
    "diagnostics"
  ],
  "repository": "https://github.com/atom-ide-community/atom-ide-diagnostics",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "atom-ide-datatip",
    "atom-ide-markdown-service",
    "busy-signal",
    "intentions",
    "linter"
  ],
  "dependencies": {
    "atom-package-deps": "^5.1.0",
    "dompurify": "^1.0.11",
    "etch": "^0.14.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/travis-cli": "^8.0.0",
    "@semantic-release/apm-config": "^6.0.1",
    "@types/atom": "^1.36.1",
    "@types/dompurify": "0.0.32",
    "@types/node": "^10.14.9",
    "atom-languageclient": "^0.9.9",
    "commitlint": "^8.0.0",
    "husky": "^2.4.1",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.16"
  },
  "release": {
    "extends": "@semantic-release/apm-config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "consumedServices": {
    "markdown-renderer": {
      "versions": {
        "0.1.0": "consumeMarkdownRenderer"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "consumeCodeActionProvider"
      }
    },
    "busy-signal": {
      "versions": {
        "1.0.0": "consumeBusySignal"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatipService"
      }
    }
  },
  "providedServices": {
    "linter-ui": {
      "versions": {
        "1.0.0": "provideLinterUI"
      }
    },
    "intentions:list": {
      "versions": {
        "1.0.0": "provideIntentionsList"
      }
    }
  }
}
